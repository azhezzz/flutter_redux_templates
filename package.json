{
  "name": "template",
  "displayName": "template",
  "description": "jupiter-template",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.33.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.GenerateReducerTemplate",
        "title": "Generate Reducer Template",
        "category": "Jupiter Template"
      },
      {
        "command": "extension.GenerateActionTemplate",
        "title": "Generate Action Template",
        "category": "Jupiter Template"
      },
      {
        "command": "extension.OpenJupiterTemplateFolder",
        "title": "Open Template Folder",
        "category": "Jupiter Template"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "extension.GenerateReducerTemplate",
          "group": "1_jupiter_template"
        },
        {
          "command": "extension.GenerateActionTemplate",
          "group": "1_jupiter_template"
        },
        {
          "command": "extension.OpenJupiterTemplateFolder",
          "group": "1_jupiter_template"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/mz": "0.0.32",
    "@types/node": "^10.12.21",
    "tslint": "^5.12.1",
    "typescript": "^3.3.1",
    "vscode": "^1.1.28"
  },
  "dependencies": {
    "i": "^0.3.6",
    "mz": "^2.7.0"
  }
}
